<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC '-//mybatis.org//DTD Mapper 3.0//EN' 'http://mybatis.org/dtd/mybatis-3-mapper.dtd'>
<mapper namespace='com.chunjae.doctormath.main.login.LoginUserMapper'>


    <select id="login" parameterType='java.util.LinkedHashMap' resultType="java.util.LinkedHashMap">
        SELECT userid, name
        FROM tmember
        WHERE userid=#{userId}
        AND password = #{password}

    </select>



</mapper>